<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>

    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>MiLinq</PackageId>
    <Version>1.0.0</Version>
    <Authors>Jhon Dev</Authors>
    <Company>Jhon Inc.</Company>
    <Description>Operadores LINQ personalizados para .NET</Description>
    <PackageTags>LINQ Extensions Functional</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/jhonenfl/MiLinq</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <Folder Include="Models\" />
  </ItemGroup>

</Project>
